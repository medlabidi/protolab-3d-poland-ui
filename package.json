{
  "name": "protolab-3d-poland-ui",
  "version": "1.0.0",
  "description": "ProtoLab 3D Printing Service - Full Stack Application",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "cd client && npm run dev",
    "dev:server": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "node scripts/build.js",
    "vercel-build": "npm run build",
    "start": "node dist/index.js",
    "install-all": "npm install && npm install --prefix client && npm install --prefix server",
    "verify-db": "ts-node verify-db.ts",
    "test": "echo \"Tests not configured yet\"",
    "lint": "cd client && npm run lint && cd ../server && npm run lint",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "clean": "rm -rf node_modules client/node_modules server/node_modules && npm install",
    "health-check": "npm run verify-db && npm run lint"
  },
  "devDependencies": {
    "@types/node": "^20.5.1",
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.7",
    "concurrently": "^9.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.0",
    "uuid": "^9.0.0"
  }
}
