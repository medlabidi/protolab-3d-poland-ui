{
  "name": "protolab-3d-poland-ui",
  "version": "1.0.0",
  "description": "ProtoLab 3D Printing Service - Full Stack Application",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "cd client && npm run dev",
    "dev:server": "cd server && npm run dev",
    "build": "npm run install-all && cd client && npm run build",
    "vercel-build": "npm install --prefix client && npm install --prefix server && cd client && npm run build",
    "sync-env": "node sync-env-to-vercel.js",
    "sync-env:preview": "node sync-env-to-vercel.js --preview",
    "deploy": "node sync-env-to-vercel.js && vercel --prod",
    "deploy:preview": "node sync-env-to-vercel.js --preview && vercel",
    "start": "node dist/index.js",
    "install-all": "npm install && npm install --prefix client && npm install --prefix server",
    "verify-db": "ts-node verify-db.ts",
    "init-db": "ts-node init-db.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:pricing": "ts-node server/src/__tests__/pricing.manual.ts",
    "test:file-analysis": "jest file-analysis.test.ts",
    "test:integration": "jest full-estimation.test.ts",
    "test:all": "jest --coverage",
    "test:e2e": "npx playwright test",
    "test:e2e:ui": "npx playwright test --ui",
    "test:e2e:headed": "npx playwright test --headed",
    "test:e2e:report": "npx playwright show-report",
    "lint": "cd client && npm run lint && cd ../server && npm run lint",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "clean": "rm -rf node_modules client/node_modules server/node_modules && npm install",
    "health-check": "npm run verify-db && npm run lint"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.6",
    "@types/nodemailer": "^7.0.4",
    "@vercel/node": "^2.3.0",
    "concurrently": "^9.0.1",
    "dotenv": "^16.3.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.450.0",
    "@aws-sdk/s3-request-presigner": "^3.450.0",
    "@radix-ui/react-slot": "^1.2.4",
    "@supabase/supabase-js": "^2.38.0",
    "@types/bcryptjs": "^2.4.6",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "google-auth-library": "^10.5.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.554.0",
    "multer": "^2.0.0",
    "nodemailer": "^7.0.10",
    "pino": "^10.1.0",
    "pino-http": "^11.0.0",
    "pino-pretty": "^10.3.1",
    "resend": "^6.5.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "uuid": "^9.0.0",
    "zod": "^3.22.4"
  }
}
