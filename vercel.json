{
  "version": 2,
  "buildCommand": "cd client && npm install && npm run build",
  "outputDirectory": "client/dist",
  "framework": null,
  "installCommand": "npm install",
  "git": {
    "deploymentEnabled": {
      "mahmoud": true
    }
  },
  "github": {
    "silent": true,
    "autoJobCancelation": true
  },
  "headers": [
    {
      "source": "/index.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/index.ts"
    },
    {
      "source": "/health",
      "destination": "/api/health.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/index.ts": {
      "memory": 1024,
      "maxDuration": 30
    },
    "api/health.ts": {
      "memory": 512,
      "maxDuration": 10
    }
  },
  "build": {
    "env": {
      "VITE_API_URL": "/api",
      "VITE_SUPABASE_URL": "https://ejauqqpatmqbxxhbmkzp.supabase.co",
      "VITE_SUPABASE_ANON_KEY": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZSIsInJlZiI6ImVqYXVxcXBhdG1xYnh4aGJta3pwIiwicm9sZSI6ImFub24iLCJpYXQiOjE3NjQwMzQ4MTEsImV4cCI6MjA3OTYxMDgxMX0.8YyeFRXCljXaUdKBDiBBxg3hjwP3haAyNitSLrq-duc"
    }
  }
}
